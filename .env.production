# Production Environment Variables for Render
PORT=3000
NODE_ENV=production

# Database (SQLite for simplicity on free tier)
DATABASE_PATH=/opt/render/project/.flowise
DATABASE_TYPE=sqlite

# Security
DEBUG=false
LOG_LEVEL=warn

# CORS (will be updated with your actual Vercel URL)
CORS_ORIGINS=https://YOUR_VERCEL_URL.vercel.app

# Optional: Add your OpenAI API key here later
# OPENAI_API_KEY=your_openai_key_here